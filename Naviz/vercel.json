{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://naviz-backend-staging.up.railway.app/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "https://naviz-backend-staging.up.railway.app/socket.io/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_SUPABASE_URL": "@vite-supabase-url",
    "VITE_SUPABASE_ANON_KEY": "@vite-supabase-anon-key",
    "VITE_SOCKET_URL": "@vite-socket-url"
  },
  "functions": {
    "src/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "framework": "vite",
  "regions": ["iad1"]
}
