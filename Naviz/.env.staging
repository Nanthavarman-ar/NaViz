# Staging Environment Configuration
NODE_ENV=staging
VITE_APP_NAME=Naviz (Staging)
VITE_APP_VERSION=1.0.0

# Supabase Configuration (Staging)
VITE_SUPABASE_URL=https://your-project-staging.supabase.co
VITE_SUPABASE_ANON_KEY=your_staging_supabase_anon_key

# Socket.io Configuration (Staging)
VITE_SOCKET_URL=https://naviz-backend-staging.up.railway.app
VITE_SOCKET_RECONNECT_ATTEMPTS=3
VITE_SOCKET_TIMEOUT=15000

# Babylon.js Configuration
VITE_BABYLON_DEBUG=false
VITE_BABYLON_ANTIALIAS=true

# API Configuration
VITE_API_BASE_URL=https://naviz-backend-staging.up.railway.app/api
VITE_API_TIMEOUT=8000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_TOOLS=false
VITE_ENABLE_PERFORMANCE_MONITORING=true

# External Services
VITE_MAPBOX_TOKEN=your_staging_mapbox_token
VITE_GOOGLE_MAPS_API_KEY=your_staging_google_maps_key

# Development
VITE_DEV_TOOLS_ENABLED=false
VITE_MOCK_API=false
