# Production Environment Configuration
NODE_ENV=production
VITE_APP_NAME=Naviz
VITE_APP_VERSION=1.0.0

# Supabase Configuration (Production)
VITE_SUPABASE_URL=https://your-project-production.supabase.co
VITE_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# Socket.io Configuration (Production)
VITE_SOCKET_URL=https://naviz-backend-prod.up.railway.app
VITE_SOCKET_RECONNECT_ATTEMPTS=3
VITE_SOCKET_TIMEOUT=10000

# Babylon.js Configuration
VITE_BABYLON_DEBUG=false
VITE_BABYLON_ANTIALIAS=true

# API Configuration
VITE_API_BASE_URL=https://naviz-backend-prod.up.railway.app/api
VITE_API_TIMEOUT=5000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_TOOLS=false
VITE_ENABLE_PERFORMANCE_MONITORING=true

# External Services
VITE_MAPBOX_TOKEN=your_production_mapbox_token
VITE_GOOGLE_MAPS_API_KEY=your_production_google_maps_key

# Development
VITE_DEV_TOOLS_ENABLED=false
VITE_MOCK_API=false
